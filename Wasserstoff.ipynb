        "# fn to set vectorization parameters\n",
        "def set_vectorization_params(text):\n",
        "  size = get_pdf_size(text)\n",
        "\n",
        "  #very short PDFs\n",
        "  if size < 100:\n",
        "    ngram_range = (1,1)\n",
        "    max_features = 5000\n",
        "\n",
        "  #short PDFs\n",
        "  elif 100 <= size < 1000:\n",
        "    ngram_range = (1,2)\n",
        "    max_features = 10000\n",
        "\n",
        "  #long PDFs\n",
        "  else:\n",
        "    ngram_range = (1,3)\n",
        "    max_features = 15000\n",
        "\n",
        "  return ngram_range, max_features"
      ]
