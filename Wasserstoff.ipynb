        "def lemma_stopword_KE(text):\n",
        "\n",
        "  # remove stop words and punctuations\n",
        "  doc = nlp(text)\n",
        "  txt = ' '.join([token.text for token in doc if not token.is_stop and not token.is_punct])\n",
        "\n",
        "  # Lemmatization\n",
        "  doc = nlp(txt)\n",
        "  txt = ' '.join([token.lemma_ for token in doc])\n",
        "\n",
        "  return txt"
      ]
